# KRINS AI Pattern Bridge Environment Configuration

# Server Configuration
PORT=3007
NODE_ENV=development

# Redis Configuration (for Coordination Memory)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=1

# Integration Endpoints
MCP_AI_TEAM_SERVER=http://localhost:3006
SEMANTIC_SEARCH_SERVER=http://localhost:3003
FASTAPI_BACKEND=http://localhost:8000
REACT_FRONTEND=http://localhost:3000

# AI Coordination Configuration
MAX_COORDINATION_SESSIONS=100
SESSION_TTL=3600
PATTERN_CACHE_SIZE=10000
MESSAGE_TTL=300000

# Event Bus Configuration
MAX_EVENT_LISTENERS=1000
EVENT_HISTORY_SIZE=10000
REPLAY_BUFFER_SIZE=1000
BATCH_SIZE=100

# Message Router Configuration
MAX_QUEUE_SIZE=10000
RETRY_ATTEMPTS=3
RETRY_DELAY=5000

# WebSocket Configuration
WS_HEARTBEAT_INTERVAL=30000
WS_CONNECTION_TIMEOUT=60000
MAX_WS_CONNECTIONS=500

# Security Configuration
API_RATE_LIMIT=500
CORS_ORIGINS=http://localhost:3000,http://localhost:5173,http://localhost:8000,http://localhost:3006,http://localhost:3003

# Logging Configuration
LOG_LEVEL=info
LOG_FILE=logs/ai-pattern-bridge.log

# Performance Configuration
MEMORY_TTL=86400
CLEANUP_INTERVAL=300000
STATS_UPDATE_INTERVAL=60000

# AI System Registration
AUTO_REGISTER_SYSTEMS=true
SYSTEM_HEALTH_CHECK_INTERVAL=120000
STALE_SYSTEM_THRESHOLD=300000