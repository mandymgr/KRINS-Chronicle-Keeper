    "jsdom": "^22.1.0",
    "msw": "^1.3.0",
    "postcss": "^8.4.27",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.5.14",
    "vitest": "^0.34.0"
