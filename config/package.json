{
  "name": "dev-memory-os-starter",
  "version": "1.0.0",
  "description": "Revolutionary AI Team Coordination System med komplett institutional memory - bygget av Krin & Mandy",
  "private": true,
  "workspaces": [
    "AI-SYSTEMS/*",
    "CORE-PLATFORM/*",  
    "TRADING-PLATFORM/*",
    "DASHBOARD/*",
    "config",
    "tools"
  ],
  "main": "index.js",
  "directories": {
    "doc": "SHARED/docs"
  },
  "scripts": {
    "build": "./SHARED/scripts/build-and-verify.sh",
    "build:quick": "./SHARED/scripts/build-and-verify.sh --quick",
    "build:railway": "bun run build:frontend && bun run build:backend",
    "build:frontend": "cd CORE-PLATFORM/frontend && bun install && bun run build",
    "build:backend": "cd CORE-PLATFORM/backend && bun install",
    "verify": "./SHARED/scripts/build-and-verify.sh",
    "start": "bun CORE-PLATFORM/backend/server.js",
    "start:production": "NODE_ENV=production bun CORE-PLATFORM/backend/server.js",
    "start:dev": "NODE_ENV=development bun CORE-PLATFORM/backend/server.js",
    "migrate": "bun CORE-PLATFORM/database/migrate.js",
    "migrate:reset": "bun CORE-PLATFORM/database/migrate.js --reset",
    "db:seed": "bun CORE-PLATFORM/database/seed.js",
    "db:init": "psql $DATABASE_URL -f CORE-PLATFORM/database/init.sql",
    "webhook": "cd AI-SYSTEMS/core && bun github-webhook-handler.js",
    "webhook:dev": "cd AI-SYSTEMS/core && PORT=3002 AUTO_CREATE_ADR=true bun github-webhook-handler.js",
    "semantic-server": "bun CORE-PLATFORM/backend/server.js",
    "semantic-server:dev": "NODE_ENV=development bun CORE-PLATFORM/backend/server.js",
    "process-adrs": "bun CORE-PLATFORM/backend/embedding/adr-batch-processor.js",
    "process-adrs:dry": "bun CORE-PLATFORM/backend/embedding/adr-batch-processor.js --dry-run",
    "test": "bun run test:backend && bun run test:frontend && bun run test:trading && bun run test:ai-systems",
    "test:backend": "cd CORE-PLATFORM/backend && bun test",
    "test:frontend": "cd CORE-PLATFORM/frontend && bun test",
    "test:trading": "cd TRADING-PLATFORM/realtime-trading-system && bun test",
    "test:ai-systems": "cd AI-SYSTEMS/krins-superintelligence-team && bun test",
    "test:integration": "bun CORE-PLATFORM/backend/test/integration.js",
    "lint": "bun run lint:backend && bun run lint:frontend && bun run lint:trading",
    "lint:backend": "cd CORE-PLATFORM/backend && bun run lint",
    "lint:frontend": "cd CORE-PLATFORM/frontend && bun run lint",
    "lint:trading": "cd TRADING-PLATFORM/realtime-trading-system && bun run lint",
    "validate-patterns": "bun SHARED/tools/validate-patterns.js",
    "validate-patterns:fix": "bun SHARED/tools/validate-patterns.js --fix",
    "create-pattern": "bun SHARED/tools/create-pattern.js",
    "coordinate-ai-patterns": "bun SHARED/tools/pattern-ai-coordinator.js",
    "precommit": "bun run validate-patterns && bun run build",
    "prepare": "chmod +x SHARED/scripts/build-and-verify.sh",
    "trading:start": "cd TRADING-PLATFORM/realtime-trading-system && bun start",
    "trading:dev": "cd TRADING-PLATFORM/realtime-trading-system && bun dev",
    "dashboard:living-spec": "cd DASHBOARD/living-spec-dashboard && bun dev",
    "dashboard:living-spec:build": "cd DASHBOARD/living-spec-dashboard && bun run build",
    "ai:coordinator": "cd AI-SYSTEMS/core && bun ai-pattern-bridge.js",
    "ai:webhook": "cd AI-SYSTEMS/core && bun github-webhook-handler.js",
    "ai:krin-startup": "cd AI-SYSTEMS/krin-personal-companion && npm run claude-code-krin",
    "ai:superintelligence": "cd AI-SYSTEMS/krins-superintelligence-team && bun start",
    "ai:mcp-team": "cd AI-SYSTEMS/mcp-ai-team && bun start",
    "install:all": "bun install && bun run install:workspaces",
    "install:workspaces": "bun install --filter='AI-SYSTEMS/*' && bun install --filter='CORE-PLATFORM/*' && bun install --filter='TRADING-PLATFORM/*'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mandymgr/Krins-Dev-Memory-OS.git"
  },
  "keywords": [
    "ai-coordination", 
    "pattern-driven-development",
    "revolutionary-development",
    "adr",
    "institutional-memory",
    "multi-terminal-ai",
    "krins-superintelligence",
    "monorepo-workspaces"
  ],
  "author": "Krin & Mandy - Revolutionary AI Development Partnership",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/mandymgr/Krins-Dev-Memory-OS/issues"
  },
  "homepage": "https://github.com/mandymgr/Krins-Dev-Memory-OS#readme",
  "devDependencies": {
    "husky": "^8.0.3",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  }
}