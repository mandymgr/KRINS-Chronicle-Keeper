{
  "name": "dev-memory-os-starter",
  "version": "1.0.0",
  "description": "Revolutionary AI Team Coordination System med komplett institutional memory - bygget av Krin & Mandy",
  "private": true,
  "workspaces": [
    "AI-SYSTEMS/*",
    "KRINS-HUB/*",  
    "DASHBOARD/*",
    "archive/*"
  ],
  "main": "index.js",
  "directories": {
    "doc": "SHARED/docs"
  },
  "scripts": {
    "build": "../SHARED/scripts/build-and-verify.sh",
    "build:quick": "../SHARED/scripts/build-and-verify.sh --quick",
    "build:railway": "bun run build:frontend && bun run build:backend",
    "build:frontend": "cd ../KRINS-HUB/frontend && bun install && bun run build",
    "build:backend": "cd ../KRINS-HUB/backend-api && bun install",
    "build:backend-api": "cd ../KRINS-HUB/backend-api && pip install -r requirements.txt",
    "verify": "../SHARED/scripts/build-and-verify.sh",
    "start": "bun ../KRINS-HUB/backend-api/server.js",
    "start:production": "NODE_ENV=production bun ../KRINS-HUB/backend-api/server.js",
    "start:dev": "NODE_ENV=development bun ../KRINS-HUB/backend-api/server.js",
    "api:prod": "cd ../KRINS-HUB/backend-api && python main.py",
    "api:dev": "cd ../KRINS-HUB/backend-api && bun server.js",
    "migrate": "bun ../KRINS-HUB/database/migrate.js",
    "migrate:reset": "bun ../KRINS-HUB/database/migrate.js --reset",
    "db:seed": "bun ../KRINS-HUB/database/seed.js",
    "db:init": "psql $DATABASE_URL -f ../KRINS-HUB/database/init.sql",
    "db:up": "cd database && docker compose up -d",
    "db:down": "cd database && docker compose down",
    "db:init-simple": "psql $DATABASE_URL -f database/init-simple.sql",
    "webhook": "cd ../AI-SYSTEMS/core && bun github-webhook-handler.js",
    "webhook:dev": "cd ../AI-SYSTEMS/core && PORT=3002 AUTO_CREATE_ADR=true bun github-webhook-handler.js",
    "semantic-server": "bun ../KRINS-HUB/backend-api/server.js",
    "semantic-server:dev": "NODE_ENV=development bun ../KRINS-HUB/backend-api/server.js",
    "process-adrs": "bun ../KRINS-HUB/backend-api/embedding/adr-batch-processor.js",
    "process-adrs:dry": "bun ../KRINS-HUB/backend-api/embedding/adr-batch-processor.js --dry-run",
    "test": "bun run test:backend && bun run test:frontend && bun run test:trading && bun run test:ai-systems",
    "test:backend": "cd ../KRINS-HUB/backend-api && bun test",
    "test:backend-api": "cd ../KRINS-HUB/backend-api && python -m pytest",
    "test:frontend": "cd ../KRINS-HUB/frontend && bun test",
    "test:trading": "cd ../archive/experimental-projects/trading-platform/realtime-trading-system && bun test",
    "test:ai-systems": "cd ../AI-SYSTEMS/krins-superintelligence-team && bun test",
    "test:integration": "bun ../KRINS-HUB/backend-api/test/basic-functionality-test.js",
    "lint": "bun run lint:backend && bun run lint:frontend",
    "lint:backend": "cd ../KRINS-HUB/backend-api && bun run lint",
    "lint:backend-api": "cd ../KRINS-HUB/backend-api && python -m flake8 .",
    "lint:frontend": "cd ../KRINS-HUB/frontend && bun run lint",
    "validate-patterns": "bun ../SHARED/tools/validate-patterns.js",
    "validate-patterns:fix": "bun ../SHARED/tools/validate-patterns.js --fix",
    "create-pattern": "bun ../SHARED/tools/create-pattern.js",
    "coordinate-ai-patterns": "bun ../SHARED/tools/pattern-ai-coordinator.js",
    "precommit": "bun run validate-patterns && bun run build",
    "prepare": "chmod +x ../SHARED/scripts/build-and-verify.sh",
    "trading:start": "cd ../archive/experimental-projects/trading-platform/realtime-trading-system && bun start",
    "trading:dev": "cd ../archive/experimental-projects/trading-platform/realtime-trading-system && bun dev",
    "dashboard:living-spec": "cd ../DASHBOARD/living-spec-dashboard && bun dev",
    "dashboard:living-spec:build": "cd ../DASHBOARD/living-spec-dashboard && bun run build",
    "ai:coordinator": "cd ../AI-SYSTEMS/core && bun ai-pattern-bridge.js",
    "ai:webhook": "cd ../AI-SYSTEMS/core && bun github-webhook-handler.js",
    "ai:krin-startup": "cd ../AI-SYSTEMS/krin-personal-companion && npm run claude-code-krin",
    "ai:superintelligence": "cd ../AI-SYSTEMS/krins-superintelligence-team && bun start",
    "ai:mcp-team": "cd ../AI-SYSTEMS/mcp-ai-team && bun start",
    "mcp:adapter": "cd ../AI-SYSTEMS/mcp-adapter && bun src/server.ts",
    "deploy:mcp-ai-team": "cd ../AI-SYSTEMS/deployment && bun deploy-mcp-ai-team.ts",
    "secureship:deploy": "cd ../AI-SYSTEMS/deployment && bun deploy-mcp-ai-team.ts",
    "secureship:scan": "cd ../AI-SYSTEMS/deployment/secops-gates && bun src/index.ts",
    "secops:scan": "cd ../AI-SYSTEMS/deployment/secops-gates && bun src/index.ts",
    "hybrid:start": "../SHARED/scripts/start-hybrid-system.sh",
    "install:all": "bun install && bun run install:workspaces",
    "install:workspaces": "bun install --filter='AI-SYSTEMS/*' && bun install --filter='KRINS-HUB/*' && bun install --filter='DASHBOARD/*'",
    "ai-systems:enforce-bun": "../SHARED/tools/ai-systems-bun-enforcer.sh",
    "ai-systems:check": "../SHARED/tools/ai-systems-bun-enforcer.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mandymgr/Krins-Dev-Memory-OS.git"
  },
  "keywords": [
    "ai-coordination", 
    "pattern-driven-development",
    "revolutionary-development",
    "adr",
    "institutional-memory",
    "multi-terminal-ai",
    "krins-superintelligence",
    "monorepo-workspaces"
  ],
  "author": "Krin & Mandy - Revolutionary AI Development Partnership",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/mandymgr/Krins-Dev-Memory-OS/issues"
  },
  "homepage": "https://github.com/mandymgr/Krins-Dev-Memory-OS#readme",
  "devDependencies": {
    "husky": "^8.0.3",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  }
}