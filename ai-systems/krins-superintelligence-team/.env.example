# Krins Superintelligence Environment Variables

# Server Configuration
PORT=3001
WEB_INTERFACE_PORT=3002
NODE_ENV=production

# AI Services
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Database Configuration (Railway PostgreSQL)
# Railway automatically provides DATABASE_URL, but you can also use individual vars
DATABASE_URL=postgresql://username:password@host:port/database
POSTGRES_URL=postgresql://username:password@host:port/database

# Local development fallback (hvis DATABASE_URL ikke er satt)
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=krins_superintelligence
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password

# Railway Database Info (automatisk satt av Railway)
# PGHOST=railway-provided-host
# PGPORT=railway-provided-port  
# PGDATABASE=railway-provided-db
# PGUSER=railway-provided-user
# PGPASSWORD=railway-provided-password

# External Services
ORCHESTRATOR_URL=http://localhost:3001

# Feature Flags
FEATURE_FLAGS_ENABLED=true
AB_TESTING_ENABLED=true
ROLLOUT_MANAGEMENT_ENABLED=true

# Security
CORS_ORIGIN=*
JWT_SECRET=your_jwt_secret_here

# Logging
LOG_LEVEL=info
LOG_TO_FILE=true